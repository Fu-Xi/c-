#include <stdio.h>
#include <stdlib.h>

int main()
{
    /*
    ╢Рс║обапм╪╟╦ё╨
                               ***
                             *     *
                            *       *
                            *       *
                            *       *
                            *       *
                            *       *
                            *       *
                            *       *
                             *     *
                               ***

    */
    int i,j;

    for(i = 0;i <= 10;i++)
    {
        for(j = 0;j <= 8;j++)
        {
            if((i == 0 || i == 10) && (j >= 3 && j <= 5))
            {
                printf("*");
            }
            else if((i == 1 || i == 9) && (j == 1 || j == 7))
            {
                printf("*");
            }
            else if((i >= 2 && i <= 8) && (j == 0 || j == 8))
            {
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
