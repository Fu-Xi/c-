#include <stdio.h>
#include <stdlib.h>

int main()
{
    /*
        语文  数学  英语
         89    87    76
         76    86    78
         89    90    92
         88    78    98
         65    75    77
         70    71    72
    */

    double socres[6][3] = {
        {89,87,76},
        {76,86,78},
        {89,90,92},
        {88,78,98},
        {65,75,77},
        {70,71,72}
    };

    printf("语文\t数学\t英语\n");
    int i,j;//循环变量

    for(i = 0;i < 6;i++)
    {
        for(j = 0;j < 3;j++)
        {
            printf("%-8.2lf",socres[i][j]);
        }
        printf("\n");
    }

    return 0;
}
